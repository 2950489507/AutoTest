[
	{
		"description":"这是一个会返回cookies的get请求",
		"request":
			{
				"uri":"/get/cookies",
				"method":"get"
			},
		 "response":
		 	{
		 		"text":"成功获取cookies.",
		 		"cookies":
		 		 	{
		 		 		"login":"true"
		 		 	}
		 	}
	},
	{
		"description":"需要带cookies的get请求",
		"request":
			{
				"uri":"/get/with/cookies",
				"method":"get",
				"cookies":
