						"login":"true"
					}
			},
		"response":
			{
				"status":"200",
				"json":
					{
						"message":"huhansan",
						"status":"1"
					}
			}
	},
		{
		"description":"post request with cookies and parameters!",
		"request":
			{
				"uri":"/post/with/cookies",
				"method":"post",
				"cookies":
					{
						"login":"true"
					},
				"json":
					{
						"name":"huhansan",
						"age":"18"
					}
			},
		"response":
			{
				"status":"200",
				"json":
					{
						"status":"1",
						"message":"success"
					}
			}
			
	}
